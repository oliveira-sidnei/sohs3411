	<queryString language="SQL">
		<![CDATA[SELECT LJ.NOME, 
LJ.CNPJ, 
L.nome as RESPONSAVEL,
A.DATAVENCIMENTO, 
sum(IA.VALOR) as valorTotal 
FROM ESS_ALUGUEL A 
INNER JOIN ESS_LOJA LJ ON A.IDLOJA = LJ.ID 
INNER JOIN ESS_ITEMALUGUEL IA ON A.ID = IA.IDALUGUEL
inner join ESS_LOJISTA L on L.id = LJ.idLojista
WHERE A.IDSTATUS != 50
and a.dataVencimento between $P{DATA_INI} 
AND  $P{DATA_FIM} 
group by lj.nome]]>
	</queryString>
